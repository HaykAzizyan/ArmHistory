{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "jsx": true,
      "useJSXTextNode": true,
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
      "no-unused-expressions": ["off"],
      "no-unused-vars": "off",
      "no-shadow": "off",
      "indent": "off",
      "react/jsx-indent": "off",
      "react/jsx-indent-props": "off"
    }
  }